<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>KGC</servlet-name>
        <servlet-class>FUZZY.KGC</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Upload</servlet-name>
        <servlet-class>FUZZY.Upload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TPA</servlet-name>
        <servlet-class>FUZZY.TPA</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>clloud</servlet-name>
        <servlet-class>FUZZY.clloud</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signup</servlet-name>
        <servlet-class>FUZZY.signup</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>KGC</servlet-name>
        <url-pattern>/KGC</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Upload</servlet-name>
        <url-pattern>/Upload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TPA</servlet-name>
        <url-pattern>/TPA</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>clloud</servlet-name>
        <url-pattern>/clloud</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signup</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
